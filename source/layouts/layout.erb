<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title>Transcribt</title>
    
    <%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Monda:700" %>
    <%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Cabin" %>
    <%= stylesheet_link_tag "style" %>

    <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js" %>
    <%= javascript_include_tag  "all" %>

  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>